{
  "name": "snap-ts",
  "version": "0.1.0",
  "description": "Snapping tools to ease data mutation for typescript",
  "private": false,
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "homepage": "https://github.com/m10rten/snap-ts#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/m10rten/snap-ts.git"
  },
  "bugs": {
    "url": "https://github.com/m10rten/snap-ts/issues"
  },
  "readme": "",
  "scripts": {
    "build": "tsup --format esm,cjs --dts",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --passWithNoTests",
    "prettier:check": "prettier --check .",
    "format": "prettier --write .",
    "generate": "echo 'Not implemented yet.'",
    "precheck": "pnpm typecheck && pnpm lint",
    "verify": "pnpm build && pnpm test",
    "release": "changeset publish",
    "change": "changeset",
    "prepare": "husky"
  },
  "type": "commonjs",
  "keywords": [],
  "author": "m10rten",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "2.27.10",
    "@eslint/js": "9.17.0",
    "@ianvs/prettier-plugin-sort-imports": "4.4.0",
    "@types/eslint__js": "8.42.3",
    "@types/jest": "29.5.14",
    "@types/node": "22.10.2",
    "eslint": "9.17.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "tsup": "8.3.5",
    "tsx": "4.19.2",
    "typescript": "5.7.2",
    "typescript-eslint": "8.18.0",
    "zod": "3.24.1"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./merge": {
      "types": "./dist/merge.d.ts",
      "require": "./dist/merge.js",
      "import": "./dist/merge.mjs"
    },
    "./group": {
      "types": "./dist/group.d.ts",
      "require": "./dist/group.js",
      "import": "./dist/group.mjs"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "merge": [
        "dist/merge.d.ts"
      ],
      "group": [
        "dist/group.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  }
}
